{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "proprietary",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.6",
        "doctrine/orm": "^3.5",
        "easycorp/easyadmin-bundle": "^4.27",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/google-mailer": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true,
        "process-timeout": 900
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "symfony serve"
        ],
        "test:phpcs" : [
            "php vendor/bin/php-cs-fixer fix --dry-run"
        ],
        "fix:phpcs" : [
            "php vendor/bin/php-cs-fixer fix"
        ],
        "test:twigcs" : [
            "vendor/bin/twig-cs-fixer lint ../"
        ],
        "fix:twigcs" : [
            "vendor/bin/twig-cs-fixer lint --fix ../"
        ],
        "test" : [
            "@test:phpcs",
            "@test:twigcs"
        ],
        "fix" : [
            "@fix:phpcs",
            "@fix:twigcs"
        ],
        "test:codeception": [
            "php vendor/bin/codecept clean",
            "php bin/console doctrine:database:drop --force --quiet --env=test",
            "php bin/console doctrine:database:create --quiet --env=test",
            "php bin/console doctrine:schema:create --quiet --env=test",
            "php vendor/bin/codecept run"
        ],
        "db" : [
            "php bin/console doctrine:database:drop --force --if-exists",
            "php bin/console doctrine:database:create",
            "php bin/console doctrine:migrations:migrate --no-interaction",
            "php bin/console doctrine:fixtures:load --no-interaction"
        ],
        "scripts-descriptions": {
            "start" : "Run the Symfony test web server",
            "test:phpcs" : "Run the PHP CS Fixer verification command.",
            "fix:phpcs" : "Run the PHP CS Fixer fix command.",
            "test:twigcs" : "Run the Twig CS Fixer verification command.",
            "fix:twigcs" : "Run the Twig CS Fixer fix command.",
            "test" : "Run the PHP CS Fixer and Twig CS Fixer verification command.",
            "fix" : "Run the PHP CS Fixer and Twig CS Fixer fix command.",
            "test:codeception": "Clean the output and generated code of Codeception, destroy and created the database for the test and run Test by Codeception",
            "db" : "Build the database"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.2",
        "codeception/module-doctrine": "^3.3",
        "codeception/module-symfony": "^3.8",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpunit/phpunit": "^11.5",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "vincentlanglet/twig-cs-fixer": "^3.10",
        "zenstruck/foundry": "^2.8"
    }
}
